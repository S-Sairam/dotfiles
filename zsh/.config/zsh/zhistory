# 1. Fix WiFi (Left: nmtui, Right: Refresh)\
cat > ~/.local/bin/sb-internet << 'EOF'\
#!/bin/sh\
case $BLOCK_BUTTON in\
    1) setsid -f alacritty -e nmtui ;;\
    3) pkill -RTMIN+1 dwmblocks ;;\
esac\
# Get WiFi Status\
CON=$(nmcli -t -f active,ssid dev wifi | grep '^yes' | cut -d: -f2)\
if [ -n "$CON" ]; then\
    echo "ïƒ§«  $CON"\
else\
    echo "ï±  Off"\
fi\
EOF\
\
# 2. Fix Power (Left: Lock, Right: Shutdown)\
cat > ~/.local/bin/sb-power << 'EOF'\
#!/bin/sh\
case $BLOCK_BUTTON in\
    1) setsid -f /home/sai/.local/bin/apple-lock ;;\
    3) systemctl poweroff ;;\
esac\
echo "âƒ¯»"\
EOF\
\
# 3. Fix Clipboard (Left: History, Right: Clear)\
cat > ~/.local/bin/sb-clipboard << 'EOF'\
#!/bin/sh\
case $BLOCK_BUTTON in\
    1) greenclip print | dmenu -l 10 -p "Clipboard" | xargs -r -0 greenclip print ;;\
    3) greenclip clear ;;\
esac\
# Count items\
COUNT=$(greenclip print | wc -l)\
echo "ðƒ¿ƒ³ƒ« $COUNT"\
EOF\
\
# Make them all executable\
chmod +x ~/.local/bin/sb-internet ~/.local/bin/sb-power ~/.local/bin/sb-clipboard\
\
# Restart dwmblocks to apply\
killall dwmblocks\
setsid dwmblocks &
# Overwrite sb-internet with clean code\
cat > ~/.local/bin/sb-internet << 'EOF'\
#!/bin/sh\
\
# Handle Clicks\
case $BLOCK_BUTTON in\
    # Left Click: Open WiFi Manager (nmtui)\
    1) setsid -f alacritty -e nmtui ;;\
    # Right Click: Refresh\
    3) pkill -RTMIN+1 dwmblocks ;;\
esac\
\
# Get WiFi Status\
CON=$(nmcli -t -f active,ssid dev wifi | grep '^yes' | cut -d: -f2)\
\
if [ -n "$CON" ]; then\
    echo "ïƒ§«  $CON"\
else\
    echo "ï±  Disconnected"\
fi\
EOF\
\
# Make it executable again just in case\
chmod +x ~/.local/bin/sb-internet\
\
# Restart blocks to clear the error\
killall dwmblocks\
setsid dwmblocks &
